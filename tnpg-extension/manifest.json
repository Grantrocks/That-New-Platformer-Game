{
  "name": "TNPG Stats",
  "description": "View all of your stats for That New Platformer Game and click play to play the game.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
      "default_icon": {
      "18": "/images/logo.png"
    }
  },
  "icons": {
    "18": "/images/logo.png"
  }
}
